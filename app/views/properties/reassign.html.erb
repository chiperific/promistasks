<div class="row valign-wrapper">
  <div class="col s8">
    <h3>Reassign Properties</h3>
  </div>
  <div class="col s4">
    <%= link_to 'Back', @return_path, class: 'btn green darken-2' %>
  </div>
</div>

<div class="row section">
  <table class="striped responsive-table">
    <thead>
      <tr>
        <th>Property</th>
        <th>Current Park</th>
        <th>New Park</th>
      </tr>
    </thead>
    <tbody>
      <%= render partial: 'reassign', collection: @properties %>
    </tbody>
  </table>
</div>

<% if @properties.empty? %>
  <%= render partial: 'admin_contact' %>
<% end %>

<div class='row' id='park_list'>
  <%= render partial: 'park', collection: @parks %>
</div>

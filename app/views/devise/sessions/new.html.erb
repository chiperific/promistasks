<h2>Log in</h2>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-inputs">
    <div class="input-field">
      <i class="material-icons prefix">alternate_email</i>
      <%= f.text_field :email, placeholder: 'email@example.com' %>
    </div>
    <div class="input-field">
      <i class="material-icons prefix">vpn_key</i>
      <%= f.password_field :password, placeholder: 'password' %>
    </div>
    <input type="hidden" value="0" name="user[remember_me]">
    <p>
      <label>
        <input type="checkbox" value="1" name="user[remember_me]" id="user_remember_me" />
        <span>Remember me</span>
      </label>
    </p>
  </div>

  <div class="form-actions row">
    <div class="col s4">
      <%= f.submit "Log in", class: 'btn green' %>
    </div>
    <div class="col s4">
      <%= link_to "Staff Sign In", user_google_oauth2_omniauth_authorize_path, class: 'btn green' %>
    </div>
    <div class="col s4">
      <%= render "devise/shared/links" %>
    </div>
  </div>
<% end %>

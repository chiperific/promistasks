<div class="row">
  <h3>Organization details:</h3>
</div>

<div class="row center-align">
  <div class="col s12">
    <table class="striped">
      <tbody>
        <% @info_hash.each do |k,v| %>
          <%= render partial: 'info_table', locals: { k: k, v: v } %>
        <% end %>
      </tbody>
    </table>
  </div>
  <div class="col s12">
    <table>
      <tbody>
        <% @rel_aoh.each do |h| %>
          <%= render partial: 'relationships', locals: { h: h } %>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
